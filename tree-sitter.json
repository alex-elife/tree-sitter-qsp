{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "qsp",
      "camelcase": "Qsp",
      "title": "QSP",
      "scope": "source.qsp",
      "file-types": [
        "qsps",
        "qsrc"
      ],
      "injection-regex": "^qsp$",
      "class-name": "TreeSitterQsp"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "QSP grammar for tree-sitter",
    "authors": [
      {
        "name": "Alexey Burlakov",
        "email": "alexey.burlakov@elife.co.jp",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/alex-elife/tree-sitter-qsp",
      "funding": ""
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "java": false,
    "node": false,
    "python": false,
    "rust": true,
    "swift": false,
    "zig": false
  }
}